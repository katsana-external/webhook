{
    "name": "laravie/webhook",
    "description": "Webhook Client for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Webhook\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "laravie/codex-common": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "php-http/curl-client": "^2.0",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "suggest": {
        "laravie/codex-security": "Allow to create webhook signature (^1.0)"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "minimum-stability": "dev"
}
